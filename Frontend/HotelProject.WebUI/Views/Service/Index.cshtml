@model List<HotelProject.WebUI.Dtos.ServiceDto.ResultServiceDto>

@{
	ViewData["Title"] = "Index";
	Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}


<table class="table table-bordered">
	<tr>
		<th>#</th>
		<th>Service Title</th>
		<th>Delete</th>
		<th>Edit</th>
	</tr>
	@foreach (var item in Model)
	{
		<tr>
			<th>@item.ServiceID</th>
			<td>@item.Title</td>
			<td><a href="/Service/DeleteService/@item.ServiceID" class="btn mb-1 btn-rounded btn-outline-danger">Delete</a></td>
			<td><a href="/Service/UpdateService/@item.ServiceID" class="btn mb-1 btn-rounded btn-outline-success">Edit</a></td>
		</tr>
	}
</table>
<a href="/Service/AddService/" class="btn mb-1 btn-rounded btn-outline-info">Add New Service</a>



